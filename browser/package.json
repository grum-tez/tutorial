{
  "name": "browser",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "if [ -n \"$CODESPACES\" ]; then vite --host; else vite; fi",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-bindings": "for file in ../blockchain/contracts/*.arl; do echo $file; echo $(basename $file .arl); completium-cli generate binding-dapp-ts $file > src/dapp_bindings/$(basename $file .arl).ts | tee; done",
    "browser-test": "ts-mocha -n loader=ts-node/esm --project tsconfig.json test/*.spec.ts"
  },
  "dependencies": {
    "@airgap/beacon-sdk": "^4.0.2",
    "@completium/dapp-ts": "^0.1.9",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@taquito/beacon-wallet": "^16.1.2",
    "@taquito/rpc": "^16.1.2",
    "@taquito/taquito": "~16.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "constate": "^3.3.2",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "latest",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "chai": "^4.3.7",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "mocha": "^10.2.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
